{
  "name": "n8n-nodes-evomi",
  "version": "1.0.0",
  "description": "n8n community node for Evomi Scraper API - Web scraping with intelligent mode selection",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "web-scraping",
    "scraper",
    "evomi",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/evomi/n8n-nodes-evomi",
  "author": {
    "name": "Evomi",
    "email": "hello@evomi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evomi/n8n-nodes-evomi.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json",
    "lint:fix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EvomiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Evomi/Evomi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "^2.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}